	pio: pinctrl@10005000 {
		compatible = "mediatek,mt6763-pinctrl";
		reg = <0 0x10005000 0 0x1000>;
		mediatek,pctl-regmap = <&syscfg_pctl_a>,
				       <&syscfg_pctl_lt>,
				       <&syscfg_pctl_lm>,
				       <&syscfg_pctl_lb>,
				       <&syscfg_pctl_bl>,
				       <&syscfg_pctl_rb>,
				       <&syscfg_pctl_rm>,
				       <&syscfg_pctl_rt>,
				       <&syscfg_pctl_tl>;
		pins-are-numbered;
		gpio-controller;
		#gpio-cells = <2>;
	};

