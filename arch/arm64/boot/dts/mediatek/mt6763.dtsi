	cpuxgpt: cpuxgpt@10200000 {
		compatible = "mediatek,mt6797-cpuxgpt";
		reg = <0 0x10200000 0 0x1000>;
		interrupts = <GIC_SPI 64 IRQ_TYPE_LEVEL_HIGH>,
