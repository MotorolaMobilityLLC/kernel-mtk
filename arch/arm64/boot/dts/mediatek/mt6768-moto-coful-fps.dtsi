&spi5 {
	#address-cells = <1>;
	#size-cells = <0>;
//        /delete-property/mediatek,pad-select;
	mediatek,pad-select = <0>, <1>;
//	mediatek,pad-select = <0>,<0>,<0>,<0>;
//        cs-gpios = <0>,<0>,<0>,<0>;
//        tee-only;
        chipone_tee@0 {
                status = "okay";
                compatible = "mediatek,finger-chipone";
                reg = <0>;
                spi-max-frequency = <8000000>;
                netlink-event = <30>;
        };

	goodix_tee@1 {
		status = "okay";
		compatible = "goodix,goodix-fp";
		reg = <1>;
		spi-max-frequency = <8000000>;
		netlink-event = <30>;
	};

};
