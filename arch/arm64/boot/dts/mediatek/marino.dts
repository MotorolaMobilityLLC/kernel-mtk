	pinctrl-names = "default", "miso_pullhigh", "miso_pulllow", "reset_high", "reset_low","fp_irq_mode", "fp_ldo1v8_off","fp_ldo1v8_on","fp_ldo3v3_on","fp_ldo3v3_off";
	pinctrl-0 = <&fingerprint_default>;
	pinctrl-1 = <&fingerprint_miso_pullhigh>;
	pinctrl-2 = <&fingerprint_miso_pulllow>;
	pinctrl-3 = <&fingerprint_reset_high>;
	pinctrl-4 = <&fingerprint_reset_low>;
	pinctrl-5 = <&gdfp_fp_irq_mode>;
    	pinctrl-6 = <&gdfp_fp_ldo1v8_off>;
