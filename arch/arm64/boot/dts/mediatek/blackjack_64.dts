		aud_extamp_gpio_high: aud_extamp_gpio_high {
			pins_gpio_high {
				pinmux = <PINMUX_GPIO153__FUNC_GPIO153>;
				out-enable;
				slew-rate = <2>;
				bias-disable;
				output-high;
			};
		};
};

