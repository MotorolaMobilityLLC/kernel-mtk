		pinctrl-1 = <&aud_extamp_gpio_high>;
		status = "okay";
	};
	
	&pio {
	
		aud_extamp_gpio_low: aud_extamp_gpio_low {
			pins_gpio_low {
				pinmux = <PINMUX_GPIO153__FUNC_GPIO153>;
				out-enable;
				slew-rate = <2>;
				bias-disable;
				output-low;
			};
