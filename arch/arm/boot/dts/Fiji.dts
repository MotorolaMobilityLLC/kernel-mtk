	pinctrl-12 = <&aud_switch_en_high>;
	pinctrl-13 = <&aud_switch_en_low>;
	pinctrl-14 = <&aud_rcv_en_high>;
	pinctrl-15 = <&aud_rcv_en_low>;
	status = "okay";
};

&pio {
	aud_clk_mosi_off: aud_clk_mosi_off {
		pins_cmd0_dat {
			pinmux = <PINMUX_GPIO136__FUNC_GPIO136>;
		};
	};

