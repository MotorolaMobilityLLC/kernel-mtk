		i2c_num				= <1>;
		i2c_addr				= <0x48 0 0 0>;
		polling_mode_ps			= <0>;
		polling_mode_als		= <1>;
		power_id				= <0xffff>;
		power_vol				= <0>;
		als_level = < 
					0  39   91    150    290    505     736    943
					0  82   174   299    582    1319    2940  3333
					0  123   264   399    627    1444   2144   2800
					0  63   144    237    445    724     991    1231
					0  72   163   258    560    1576   2400   3152
					0  112   240   475    746    1719   2552   3333
					  >;
		als_value = < 0  133  304  502   1004  2005  3058   4000 >;
		state_val		=  <0>;
		psctrl_val		=  <0x31>;
		alsctrl_val	= <0x38>;
