&spi1 {
	#address-cells = <1>;
	#size-cells = <0>;
	mediatek,pad-select = <0>, <1>;
	fpc_tee@0 {
		compatible = "fpc,fpc_spi";
		reg = <0>;
		fpc,enable-wakeup;
		spi-max-frequency = <8000000>;
		netlink-event = <31>;
		status = "okay";
	};

	goodix_tee@1 {
		status = "okay";
		compatible = "goodix,goodix-fp";
		reg = <1>;
		spi-max-frequency = <8000000>;
		netlink-event = <30>;
	};
};
